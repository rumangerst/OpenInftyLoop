; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="OpenInftyLoop"
run/main_scene="res://Game.tscn"
boot_splash/image="res://gfx/splash.png"
boot_splash/bg_color=Color( 0.631373, 0.384314, 0.815686, 1 )
config/icon="res://icon.png"

[display]

window/dpi/allow_hidpi=true
window/handheld/orientation="sensor_portrait"

[gui]

theme/use_hidpi=true

[locale]

translations=PoolStringArray( "res://translations.en.translation", "res://translations.de.translation" )
locale_filter=[ 0, [  ] ]

[rendering]

environment/default_environment="res://default_env.tres"
